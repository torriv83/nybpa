{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "althinect/filament-spatie-roles-permissions": "^2.0",
        "bezhansalleh/filament-panel-switch": "^1.0.6",
        "calebporzio/sushi": "^2.4",
        "doctrine/dbal": "^3.0",
        "filament/filament": "^v3.0.22",
        "guzzlehttp/guzzle": "^7.2",
        "hugomyb/filament-error-mailer": "^3.1",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.7",
        "league/flysystem-sftp-v3": "3.0",
        "livewire/livewire": "^3.0.0",
        "maatwebsite/excel": "^3.1",
        "rickdbcn/filament-email": "^1.0",
        "saade/filament-fullcalendar": "^v3.0.0-beta1",
        "sentry/sentry-laravel": "^4.0",
        "shuvroroy/filament-spatie-laravel-backup": "^2.1",
        "stechstudio/filament-impersonate": "^3.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "fakerphp/faker": "^1.9.1",
        "laradumps/laradumps": "^4.0",
        "larastan/larastan": "^v3.0",
        "laravel/pint": "^1.22",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^v8.1",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-livewire": "^4.0",
        "pestphp/pest-plugin-type-coverage": "^4.0",
        "pestphp/pest-plugin-browser": "^4.0",
        "phpmd/phpmd": "^2.13.0",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^12.3",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": []
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpstan": [
            "./vendor/bin/phpstan analyse"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
